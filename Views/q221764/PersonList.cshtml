@model List<TAPI_Interviewer.Models.SP_q221764_GETPERSONLISTResult>

<!DOCTYPE html>

<html>
<head>
    @{
        ViewBag.Title = "지원 사업 조사 - 응답자 리스트";
        ViewBag.idx = 1;

             


    }

    <script type="text/javascript">


        function goSurvey(pid)
        {
            window.location.href = './Survey?pid='+pid;
        }

        function goContact(pid) {
            window.location.href = './Contact?pid=' + pid;
        }

        $(document).ready(function () {



            $('#exit').click(function () {
                window.location.href = './List'
            });

        
        });




        function isTelePhone(p) {
            p = p.split('-').join('');

            var regPhone = /^(0(2|3[1-3]|4[1-4]|5[1-5]|6[1-4]|70|505))[1-9]+[0-9]{6,7}$/;

            return regPhone.test(p);
        }

        function isCellPhone(p) {
            p = p.split('-').join('');

            var regPhone = /^((01[1|6|7|8|9])[1-9]+[0-9]{6,7})|(010[1-9][0-9]{7})$/;

            return regPhone.test(p);
        }

    </script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <link href="@Url.Content("../../Content/q210801/PanelInfo.css?version=4")" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="../../Scripts/RPS_Script.js"></script>
</head>
<body>
    <div style="width : 100%;display: inline-block;">
        @Html.Partial("CommonAddressSearchScript")

        <table class="infoTable">
            <tr>
                <td class="tdValue" colspan="2">
                    수행기관 정보
                </td>
                
            </tr>
            <tr>
                <td class="tdValue">
                    OID
                </td>
                <td class="tdValue3">
                    @(ViewBag.OID)
                </td>
            </tr>

            <tr>
                <td class="tdValue">
                    기관유형
                </td>                
                <td class="tdValue3" >
                    @(ViewBag.기관유형)
                </td>
            </tr>
            <tr>
                <td class="tdValue">
                    수행기관명
                </td>
                <td class="tdValue3">
                    @(ViewBag.수행기관명)
                </td>
            </tr>

        </table>
        <div class="buttonDiv">
           
        </div>
       
     
        <div style="overflow-x:scroll;width:100%;">
            @Html.Action("PersonListPartial", "q221764", new {keyid=@ViewBag.OID })            
        </div>
        
    </div>
</body>
</html>