        function saveInfo(href, onlySave, sendMsg) {



            var age = $("select[name='age'] option:selected").val();
            var gender = $(":radio[name='gender']:checked").val();
            var tel1 = $("select[name='tel1'] option:selected").val();
            if(sendMsg == true)
            {
                if( tel1 != '010')
                {
                    alert('문자 발송은 휴대전화 번호만 가능합니다.');
                    return;
                }
            }


            var tel = tel1;

            var cname = $("#changeCname").val();
            var pname = $("#pname").val();

            var cType = $(":radio[name='cType']:checked").val();
            var cTypeEtc = $("#cTypeEtc").val();
            var area = $("select[name='area'] option:selected").val();
            var memberC = $("select[name='memberC'] option:selected").val();

            var positionType = $(":radio[name='positionType']:checked").val();
            var positionTypeEtc = $("#positionTypeEtc").val();


            if (cname == '' && $('#IschangeCname').is(':checked')) {
                $("#changeCname").focus();
                alert('변경 된 사업체명을 입력 해 주세요.');
                return;
            }

            var address = '';
            var btype = '';
            if('@Model.생성여부' == '생성')
            {
                cname = $("#cname").val();
                if( cname == '')
                {
                    alert('사업체명을 입력 해 주세요.');
                    return;
                }

                
                btype = $(":radio[name='bType']:checked").val();
                if( btype == '' || btype == undefined )
                {
                    alert('업종을 선택해 주세요.');
                    return;
                }

                address = $("#roadAddrPart1").val();
                if( address == '' && '@Model.주소' == '')
                {
                    alert('사업체 주소를 입력해 주세요.');
                    return;
                }
                var sebuAddr = $("#sebuAddr").val();
                if( sebuAddr != '')
                {
                    address = address + ' ' + sebuAddr;
                }

            }

            if (pname == '') {
                $("#pname").focus();
                alert('이름을 입력 해 주세요.');
                return;
            }

            if (pname.length > 50) {
                $("#pname").focus();
                alert('이름을 50자 이내로 입력 해 주세요.');
                return;
            }

            if (age == '' )
            {
                $('#age').focus();
                alert("응답자 나이를 선택 해 주세요.")
                return;
            }

            if (gender == undefined || gender == '')
            {
                alert('성별을 선택 해 주세요.');
                return;
            }



            if (tel1=='') {
                alert('전화번호를 입력해 주세요.');
                return;
            }

            if (tel1 != '없음'  && tel1 != '') {
                tel = tel1 + '-' + $("#tel2").val() + '-' + $("#tel3").val();

                if ( tel != '02-000-0000' && tel != '02-0000-0000' && (isTelePhone(tel) == false && isCellPhone(tel) == false)) {
                    $("#tel2").focus();
                    alert('유효한 전화 번호를 입력 해 주세요.');
                    return;
                }
            }

            //if (hp1 != '없음' && hp1 != '') {
            //    hp = hp1 + '-' + $("#hp2").val() + '-' + $("#hp3").val();

            //    if (hp != '010-0000-0000' && isCellPhone(hp) == false) {
            //        $("#hp2").focus();
            //        alert('유효한 휴대전화 번호를 입력 해 주세요.');
            //        return;
            //    }
            //}

            if (cType == undefined || cType == "")
            {
                alert("사업체 형태를 선택 해 주세요.");
                $('#cType').focus();
                return;
            }

            if (cType == "기타" && $.trim(cTypeEtc) == "")
            {
                $('#cTypeEtc').focus();
                alert("사업체 형태 기타 내용을 입력 해 주세요.");
                return;
            }

            if (area == "" )
            {
                alert("사업장 면적을 선택 해 주세요.");
                $('#cType').focus();
                return;
            }

            if (memberC == '' )
            {
                $('#memberC').focus();
                alert("종업원 수를 선택 해 주세요.")
                return;
            }
            if (positionType == undefined || positionType == "")
            {
                alert("입지 유형을 선택 해 주세요.");
                $('#positionType').focus();
                return;
            }

            if (positionType == "기타" && $.trim(positionTypeEtc) == "")
            {
                $('#positionTypeEtc').focus();
                alert("입지 유형 기타 내용을 입력 해 주세요.");
                return;
            }


            var aType = $(":radio[name='aType']:checked").val();
            if( onlySave == false && (aType == undefined || aType == ""))
            {
                alert("조사 방법을 선택 해 주세요.");
                $('#aType').focus();
                return;
            }
            else if(aType == undefined )
            {
                aType = '';
            }

            $.post("./SaveInfo", { "keyid": @(Model.상권ID), "pid": @(Model.PID), "changeCompanyName":cname, "name": pname, "age":age, "gender": gender, "tel": tel, "companyType":cType,"companyTypeEtc":cTypeEtc, "companyArea":area, "memberCount":memberC, "positionType":positionType, "positionTypeEtc":positionTypeEtc , "atype":aType, "isSend":sendMsg, "address":address , "btype":btype}, function (result) {
                if (result != '')
                {
                    alert(result);
                }
                else
                {
                    if(sendMsg == true)
                    {
                        alert('문자 발송 완료');
                    }
                    window.location.href = href;
                }
            }).error(function (jqXHR, status, error) {
                alert(status);
                alert(error);
            });
        }

        function isTelePhone(p) {
            p = p.split('-').join('');

            var regPhone = /^(0(2|3[1-3]|4[1-4]|5[1-5]|6[1-4]|70|505))[1-9]+[0-9]{6,7}$/;

            return regPhone.test(p);
        }

        function isCellPhone(p) {
            p = p.split('-').join('');

            var regPhone = /^((01[1|6|7|8|9])[1-9]+[0-9]{6,7})|(010[1-9][0-9]{7})$/;

            return regPhone.test(p);
        }


