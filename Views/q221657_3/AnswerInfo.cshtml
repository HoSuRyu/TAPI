                    alert('해당 업종은 더이상 조사할 수 없습니다.');
                    return;
                }
                saveInfo('./Survey?keyid=@(Model.상권ID)&pid=@(Model.PID)', false, false);
            });

            $('#sendMsg').click(function(){
                if( '@(Model.진행상태)' == '완료')
                {
                    alert('설문을 완료한 응답자 입니다.');
                    return;
