@model List<TAPI_Interviewer.Models.SP_q220677_GETLISTResult>


<script type="text/javascript">

    $(document).ready(function () {
        setStatus();
    });

    function goAnswerInfo(pid, status) {
        if (status == '대체요청') {
            alert('대체요청 하셨습니다.');
            return;
        }

        if (status == '이관요청') {
            alert('이관요청 하셨습니다.');
            return;
        }

        if (pid != null) {
            window.location.href = './Select?pid=' + pid;
        }
    }

    function setStatus() {

        $.post("./ProgressPartial", null, function (result) {
            $("#Progress").replaceWith(result);
        });
    }
   

    function Refresh() {
        location.reload();
    }
</script>

@Html.Partial("header")

<div id="contwrap">
    <div class="contentbox">
        <div class="boardbox">
            <div id="Progress"></div>

                <div class="bd_contbox">
                    <div class="tablebox" style="overflow-x:scroll;width:100%;">
                        @Html.Partial("ListPartial", Model)
                    </div>
                </div>
            </div>

        </div>
        @Html.Partial("footer")
    </div>


    </body>
</html>
