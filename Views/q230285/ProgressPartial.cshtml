@model TAPI_Interviewer.Models.Project.ProgressCount

<div class="bd_titbox">
   
    <div class="filterbox">
        @{
            if (Model == null)
            {
                <div class="txt_prog">배부: 00</div>
                <div class="txt_prog">진행: 00</div>
                <div class="txt_prog">완료: 00</div>
            }
            else
            {
                <div class="txt_prog">배부: @Model.All</div>
                <div class="txt_prog">진행: @Model.Ing</div>
                <div class="txt_prog">완료: @Model.End</div>
            }
        }
    </div>
</div>

