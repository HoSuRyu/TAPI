@Html.DevExpress().GridView(
    settings =>
    {
        settings.Name = "List";
        settings.KeyFieldName = "집계구번호";
        settings.CallbackRouteValues = new { Controller = "q220850", Action = "ListPartial" };
        settings.Width = System.Web.UI.WebControls.Unit.Pixel(1600);



