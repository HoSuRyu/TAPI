                                <td>
                                    <input type="text" class="form_ctrl" style="width:80%;" id="etc" name="etc">
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="bot_btnbox">
                    <button type="button" class="btnsty01" id="ok">대체 요청</button>
                    <button type="button" class="btnsty03" id="cancel">취소</button>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("footer")
